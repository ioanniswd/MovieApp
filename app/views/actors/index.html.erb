<div class="main actor-index">
  <div class="container">
    <div class="row">

      <% @actors.each do |actor| %>
        <div class="actor col-xs-2">
          <%= link_to actor do %>
            <img src=<%= actor.image %> alt="">
            <h3><%= actor.first_name %> <%= actor.last_name %></h3>
            <p><%= actor.bio %></p>
          <% end %>
        </div>
      <% end %>
    </div>
  </div>
</div>
